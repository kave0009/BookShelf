{
  "name": "root",
  "version": "0.1.0",
  "scripts": {
    "build:frontend": "cd frontend && npm install && npm run build",
    "start:frontend": "cd frontend && npm start",
    "start:backend": "cd backend && npm install && nodemon server.mjs",
    "start:prod": "npm-run-all build:frontend serve:frontend start:backend",
    "serve:frontend": "cd frontend && npx serve -s build",
    "start": "npm-run-all --parallel start:backend start:frontend"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-rate-limit": "^5.2.6",
    "helmet": "^4.4.1",
    "open": "^10.1.0",
    "pg": "^8.5.1",
    "serve": "^14.0.1",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5"
  }
}
